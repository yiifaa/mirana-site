<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
 	 <diskStore path="java.io.tmpdir/ehcache"/>
 	 <defaultCache 	maxElementsInMemory="1000" 
 	 			   	eternal="false"
           			timeToIdleSeconds="1200" 
           			timeToLiveSeconds="1200" 
           			overflowToDisk="false"
           			clearOnFlush="true">
      </defaultCache>
</ehcache>